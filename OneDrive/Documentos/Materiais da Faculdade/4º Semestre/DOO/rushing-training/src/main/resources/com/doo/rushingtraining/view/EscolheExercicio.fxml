<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="670.0" prefWidth="370.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.doo.rushingtraining.controller.ExercicioController">
   <children>
      <Label layoutX="25.0" layoutY="27.0" prefHeight="32.0" prefWidth="112.0" text="Exercicios" textAlignment="CENTER" AnchorPane.leftAnchor="25.0">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <TableView fx:id="tbExercicios" layoutX="20.0" layoutY="80.0" prefHeight="517.0" prefWidth="333.0">
         <columns>
            <TableColumn fx:id="clmnNome" prefWidth="130.0" text="Nome" />
            <TableColumn fx:id="clmnRepeticoes" prefWidth="85.0" text="Aparelho" />
            <TableColumn fx:id="clmnPeso" minWidth="0.0" prefWidth="57.0" text="Peso Min" />
            <TableColumn fx:id="clmnPeso1" prefWidth="60.0" text="Peso Max" />
         </columns>
      </TableView>
      <Button fx:id="btnEditaExercicio" layoutX="23.0" layoutY="616.0" mnemonicParsing="false" onAction="#selecionarExercicio" prefHeight="46.0" prefWidth="95.0" text="Editar Exercício" textAlignment="CENTER" wrapText="true" />
      <Button fx:id="btnSair" layoutX="255.0" layoutY="616.0" mnemonicParsing="false" onAction="#sair" prefHeight="46.0" prefWidth="95.0" text="Sair" />
      <Button fx:id="btnExcluiExercicio" layoutX="138.0" layoutY="616.0" mnemonicParsing="false" onAction="#excluir" prefHeight="46.0" prefWidth="95.0" text="Excluir Exercício" textAlignment="CENTER" textFill="RED" wrapText="true" />
   </children>
</AnchorPane>
